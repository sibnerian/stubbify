{
  "name": "stubbify",
  "version": "0.1.0",
  "description": "Stubbify files so that some code isn't included.",
  "main": "lib/stubbifier.js",
  "scripts": {
    "test": "eslint lib/*.js cli.js test/*.js --rulesdir .eslint_rules && mocha"
  },
  "keywords": [
    "stubbify",
    "CIS197"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/isibner/stubbify.git"
  },
  "author": "Ian Sibner",
  "license": "MIT",
  "dependencies": {
    "commander": "2.5.1",
    "glob": "4.3.2",
    "highland": "git://github.com/caolan/highland.git#5e0b846",
    "mkdirp": "0.5.0"
  },
  "devDependencies": {
    "chai": "1.10.0",
    "del": "1.1.1",
    "mocha": "2.1.0"
  },
  "preferGlobal": true,
  "bin": {
    "stubbify": "./cli.js"
  }
}
